<?xml version='1.0' encoding='utf-8'?>
<widget id="com.gpc.Studio120Test2" version="3.2.6" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Studio 120 Test</name>
    <description>
      TEST- Push nuove
    </description>
    <author email="info@gpchannel.com" href="http://www.gpchannel.com">
        Global Product Channel
    </author>
    <gap:plugin name="org.apache.cordova.device" />
    <gap:plugin name="org.apache.cordova.inappbrowser" source="npm" />
    <gap:plugin name="org.apache.cordova.core.vibration" />
    <gap:plugin name="com.adobe.plugins.gaplugin" />
    <gap:plugin name="cordova-plugin-whitelist" source="npm" version="1.0.0" />
    <allow-navigation href="*" />
    <allow-intent href="*" />
    <access origin="*" />
    <feature name="PushPlugin">
        <param name="ios-package" value="PushPlugin" />
    </feature>
    <feature name="Notification">
        <param name="ios-package" value="CDVNotification" />
    </feature>

    <preference name="android-minSdkVersion" value="19"/>
    <preference name="permissions" value="none" />
    <preference name="orientation" value="portrait" />
    <preference name="target-device" value="universal" />
    <preference name="fullscreen" value="true" />
    <preference name="webviewbounce" value="true" />
    <preference name="prerendered-icon" value="true" />
    <preference name="stay-in-webview" value="false" />
    <preference name="ios-statusbarstyle" value="black-opaque" />
    <preference name="detect-data-types" value="true" />
    <preference name="exit-on-suspend" value="false" />
    <preference name="show-splash-screen-spinner" value="true" />
    <preference name="auto-hide-splash-screen" value="true" />
    <preference name="disable-cursor" value="false" />
    <preference name="android-minSdkVersion" value="7" />
    <preference name="android-installLocation" value="auto" />
    <preference name="phonegap-version" />
    <gap:config-file parent="UIStatusBarHidden" platform="ios">
        <true />
    </gap:config-file>
    <gap:config-file parent="UIViewControllerBasedStatusBarAppearance" platform="ios">
        <false />
    </gap:config-file>
    <icon src="icons.png" />
    <icon gap:density="ldpi" gap:platform="android" src="res/icons/android/icon-36-ldpi.png" />
    <icon gap:density="mdpi" gap:platform="android" src="res/icons/android/icon-48-mdpi.png" />
    <icon gap:density="hdpi" gap:platform="android" src="res/icons/android/icon-72-hdpi.png" />
    <icon gap:density="xhdpi" gap:platform="android" src="res/icons/android/icon-96-xhdpi.png" />
    <icon gap:platform="blackberry" src="res/icons/blackberry/icon-80.png" />
    <icon gap:platform="blackberry" gap:state="hover" src="res/icons/blackberry/icon-80.png" />
    <icon gap:platform="ios" height="48" src="res/icons/ios/icon-48.png" width="48" />
    <icon gap:platform="ios" height="57" src="res/icons/ios/icon-57.png" width="57" />
    <icon gap:platform="ios" height="60" src="res/icons/ios/icon-60.png" width="60" />
    <icon gap:platform="ios" height="72" src="res/icons/ios/icon-72.png" width="72" />
    <icon gap:platform="ios" height="76" src="res/icons/ios/icon-76.png" width="76" />
    <icon gap:platform="ios" height="114" src="res/icons/ios/icon-57-2x.png" width="114" />
    <icon gap:platform="ios" height="120" src="res/icons/ios/icon-60-2x.png" width="120" />
    <icon gap:platform="ios" height="144" src="res/icons/ios/icon-72-2x.png" width="144" />
    <icon gap:platform="ios" height="152" src="res/icons/ios/icon-76-2x.png" width="152" />
    <icon gap:platform="ios" height="512" src="res/icons/ios/icon-512.png" width="512" />
    <icon gap:platform="ios" height="1024" src="res/icons/ios/icon-1024.png" width="1024" />
    <icon gap:platform="webos" src="res/icons/webos/icon-64.png" />
    <icon gap:platform="winphone" src="res/icons/windows-phone/icon-62.png" />
    <icon gap:platform="winphone" gap:role="background" src="res/icons/windows-phone/icon-173.png" />
    <gap:splash gap:density="ldpi" gap:platform="android" src="res/screen/android/screen-ldpi-portrait.png" />
    <gap:splash gap:density="mdpi" gap:platform="android" src="res/screen/android/screen-mdpi-portrait.png" />
    <gap:splash gap:density="hdpi" gap:platform="android" src="res/screen/android/screen-hdpi-portrait.png" />
    <gap:splash gap:density="xhdpi" gap:platform="android" src="res/screen/android/screen-xhdpi-portrait.png" />
    <gap:splash gap:platform="blackberry" src="res/screen/blackberry/screen-225.png" />
    <gap:splash gap:platform="ios" height="480" src="res/screen/ios/screen-iphone-portrait.png" width="320" />
    <gap:splash gap:platform="ios" height="320" src="res/screen/ios/screen-iphone-landscape.png" width="480" />
    <gap:splash gap:platform="ios" height="960" src="res/screen/ios/screen-iphone-portrait-2x.png" width="640" />
    <gap:splash gap:platform="ios" height="1136" src="res/screen/ios/screen-iphone-portrait-568h-2x.png" width="640" />
    <gap:splash gap:platform="ios" height="1004" src="res/screen/ios/screen-ipad-portrait.png" width="768" />
